{
    "manifest_version": 3,
    "name": "arXiv HTML UI Toggle",
    "version": "1.0",
    "description": "Hides UI elements on arXiv HTML pages with a toggle button.",
    "content_scripts": [
        {
            "matches": [
                "https://arxiv.org/html/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ]
}